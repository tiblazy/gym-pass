generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Member {
  id String @id @default(uuid())

  username String
  email    String
  password String

  avatar     String
  created_at DateTime
  updated_at DateTime

  is_active     Boolean
  validate_totp DateTime

  @@map("members")
}
