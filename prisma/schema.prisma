generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Member {
  id String @id @default(uuid())

  username String
  email    String @unique
  password String

  avatar     String?
  created_at DateTime  @default(now())
  updated_at DateTime?

  totp_created_at DateTime @unique @default(now())
  totp_key String 
  is_active       Boolean  @default(false)

  @@map("members")
}
